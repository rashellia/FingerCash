<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fingercash.app.fingercash">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fingerprint"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomePage" />
        <activity android:name=".LoginPage" />
        <activity android:name=".HomePage" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".RegisterPage2" />
        <activity android:name=".CheckBalance" />
        <activity android:name=".Profile" />
        <activity android:name=".RegisterRequest" />
        <activity android:name=".SessionManager" />
        <activity android:name=".JSONParser" />
        <activity android:name=".AppController" />
        <activity android:name=".Server" />
        <activity android:name=".Help" />
        <activity android:name=".Help2"></activity>
    </application>

</manifest>